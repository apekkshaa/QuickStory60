  <h2 class="quick-story">QuickStory60</h2>
  <div class="capture-share-connect">
    <!-- Each word in a separate span for animation -->
    <span class="word capture">#CAPTURE</span>
    <span class="word share">#SHARE</span>
    <span class="word connect">#CONNECT</span>
  </div>
  <div class="container">
    <div class="content">
      <!--logo-->
      <img src="/images/NAVQS.PNG" alt="pin logo" class="img1" />
      <p class="header"> Sign In</p>

      <form action="/register" method="post">
        <input type="email" name="email" placeholder="Email" class="detail" required /><br />
        <input type="text" name="username" placeholder="Username" class="detail" required /><br />
        <input type="text" name="fullname" placeholder="Full Name" class="detail" required /><br />
        <input type="password" name="password" placeholder="Password" class="detail" required />
        <input type="submit" class="btn int" value="Register">
      </form>

      <footer>
        <p>
          By continuing, you agree to QuickStory60's
          <b>Terms of Service, Privacy policy.</b>
        </p>
        <hr />
        <a href="/login" class="forgot-password">Already on QuickStory60? Login</a>
      </footer>
    </div>
